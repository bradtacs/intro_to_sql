
1 SELECT * FROM robots WHERE source = 'The Hitchhiker''s Guide to the Galaxy';

2. SELECT * FROM robots WHERE personality = 'anxious';

3. SELECT * FROM recipes WHERE nut_free = true;

4 SELECT COUNT(*) FROM recipes WHERE gluten_free = true AND vegetarian = false;

5. SELECT MAX(number_of_legs) FROM animals;

6. SELECT MAX(minutes_required) FROM recipes;

7.SELECT * FROM robots WHERE name ILIKE 'M%';

8. SELECT COUNT(*)  FROM board_games WHERE min_players < 9 AND max_players >= 8;

9. SELECT * FROM animals WHERE swimming AND egg_laying = true;

10 SELECT * FROM animals WHERE swimming AND egg_laying = true AND flying = false;

11 SELECT name, max_players FROM board_games ORDER BY max_players DESC LIMIT 1;
